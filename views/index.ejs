<% include partials/header %>
<div class="hero-section">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                    <h1 class="hero-title">Apartments In Your Area Dallas</h1>
                    <p class="hero-text"><strong>Your Types. Your Style. Your Color.</strong> </p>
                    <a href="#" class="btn btn-default">Your slider buttons</a> </div>
            </div>
        </div>
    </div>
    <div class="row jumbotron" id='apt'>
    <% apt.forEach(apt => {%>
        <div class="col-md-4 col-sm-6">
          <div class="thumbnail card">
            <img src="uploads/<%= apt.image  %>" class="responsive-img rounded img-fluid img-thumbnail">            <div class="caption">
              <h4 class="caption"><%= apt.title %></h4>
            </div>
            <p>
              <a class="btn btn-primary btn-sm btn-b-mg" href="/campgrounds/<%= apt._id %>">
                More Info
              </a>
            </p>
          </div>
        </div>
      <% })%>
    </div>
<% include partials/footer %>